{
  "manifestVersion": 1,
  "id": "nexus-iq-azure-devops-extension",
  "version": "68.0.1",
  "name": "Nexus IQ for Azure DevOps",
  "description": "Nexus IQ policy evaluations for Azure DevOps",
  "publisher": "MauryCupitt",
  "targets": [
    {
    "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "/iqtask/images/icon.png"
  },
  "scopes": [
    "vso.work"
  ],
  "categories": [
        "Azure Pipelines"
  ],
  "files": [
    { "path": "iqtask" }
  ],
  "contributions": [
		{
			"id": "nexus-iq-vsts-build-task",
			"type": "ms.vss-distributed-task.task",
			"targets": [
				"ms.vss-distributed-task.tasks"
			],
			"properties": {
				"name": "iqtask"
			}
    }
  ]
}
